def linear_search(arr, target):
    index = 0
    for value in arr:
        if value == target:
            return index
        index += 1
    return -1
numbers = [10, 20, 30, 40, 50]
target = 30

result = linear_search(numbers, target)

if result != -1:
    print(f"Element {target} found at index {result}")
else:
    print(f"Element {target} not found in the list")
